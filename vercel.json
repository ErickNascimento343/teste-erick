{
    "builds": [
      {
        "src": "src/public/views/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/public/views/index.html"
      }
    ]
  },
{
  "installCommand": "npm install -D tailwindcss npx tailwindcss init"
}
  
